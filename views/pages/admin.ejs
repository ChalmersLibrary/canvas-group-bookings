<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header', { user: session.user }); %>
</header>

<main>
    <div class="container-fluid py-2">
        <h1 class="display-5">Administration</h1>
    </div>

    <div class="container-fluid py-2">
        <div class="card">
            <div class="card-header">
                Things
            </div>
            <ul class="list-group list-group-flush">
                <li class="list-group-item">Tillf√§llen</li>
                <li class="list-group-item">Available times</li>
                <li class="list-group-item">Teachers</li>
                <li class="list-group-item">Lokaler</li>
            </ul>
        </div>
    </div>
</main>

<section>
    <div class="container-fluid py-2">
        <%- include('../partials/slots', { }); %>
    </div>
    
    <p>Groups you are currently member of:</p>
    <ul>
        <% groups.forEach(function(group) { %>
            <li>
                <strong><%= group.name %></strong>
            </li>
        <% }); %>
    </ul>
</section>

<section>
    <p>Session information:</p>
    <code><%= JSON.stringify(session) %></code>
</section>

<footer>
  <%- include('../partials/footer', { version: version }); %>
</footer>

</body>
</html>